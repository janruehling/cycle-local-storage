    "prod:serve": "pm2 reload webapp || pm2 start app.js --name webapp",
    "dev": "npm run clean && parallelshell 'npm run dev:serve -s'",
    "prod": "npm run clean && npm run prod:build && npm run prod:serve",
    "copy:cq-prolyfill": "cp node_modules/cq-prolyfill/cq-prolyfill.min.js dist/lib/",
    "clean": "rm -rf dist/* && mkdir dist/lib",

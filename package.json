{
  "name": "medical.zipwire.com",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "license": "ISC",
  "main": "app.js",
  "scripts": {
    "e2e": "npm run prod && npm run e2e:start && npm run e2e:stop || npm run e2e:stop",
    "e2e:only": "npm run prod:serve && npm run e2e:start && npm run e2e:stop || npm run e2e:stop",
    "e2e:start": "nightwatch -c tests/e2e/nightwatch.json",
    "e2e:stop": "(se stop && pm2 stop webapp)",
    "dev:serve": "webpack-dev-server --inline --hot",
    "prod:build": "webpack --optimize-minimize --optimize-occurence-order --optimize-dedupe",
    "prod:serve": "pm2 restart webapp || pm2 start app.js --force --name webapp",
    "dev": "npm run clean && npm run copy && parallelshell 'npm run dev:serve -s'",
    "prod": "npm run clean && npm run copy && npm run prod:build && npm run prod:serve",
    "copy": "npm run copy:rx && npm run copy:history && npm run copy:cycle && npm run copy:cycleDOM && npm run copy:cycleHTTP && npm run copy:cycleIsolate && npm run copy:ramda && npm run copy:moment && npm run copy:normalize && npm run copy:cropperjs && npm run copy:jquery",
    "copy:rx": "cp node_modules/rx/dist/rx.all.min.js dist/lib/",
    "copy:history": "cp node_modules/history/umd/history.min.js dist/lib/",
    "copy:cycle": "cp node_modules/@cycle/core/dist/cycle.min.js dist/lib/",
    "copy:cycleDOM": "cp node_modules/@cycle/dom/dist/cycle-dom.min.js dist/lib/",
    "copy:cycleHTTP": "cp node_modules/@cycle/http/dist/cycle-http-driver.min.js dist/lib/",
    "copy:cycleIsolate": "cp node_modules/@cycle/isolate/dist/cycle-isolate.min.js dist/lib/",
    "copy:jquery": "cp node_modules/jquery/dist/jquery.min.js dist/lib/",
    "copy:cropperjs": "cp node_modules/cropperjs/dist/cropper.min.js dist/lib/ && cp node_modules/cropperjs/dist/cropper.min.css dist/css/",
    "copy:ramda": "cp node_modules/ramda/dist/ramda.min.js dist/lib/",
    "copy:moment": "cp node_modules/moment/min/moment.min.js dist/lib/",
    "copy:normalize": "cp node_modules/normalize-css/normalize.css dist/css/",
    "copy:cq-prolyfill": "cp node_modules/cq-prolyfill/cq-prolyfill.min.js dist/lib/",
    "clean": "rm -rf dist/* && mkdir dist/lib && mkdir dist/css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "chai-virtual-dom": "^1.0.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.10.0",
    "json-loader": "^0.5.4",
    "nightwatch": "^0.8.18",
    "parallelshell": "^2.0.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.9.1",
    "postcss-modules": "^0.4.1",
    "postcss-modules-values": "^1.1.2",
    "postcss-nested": "^1.0.0",
    "se-cli": "^0.1.2",
    "style-loader": "^0.13.0",
    "wallaby-webpack": "0.0.22",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "@cycle/core": "^6.0.3",
    "@cycle/dom": "^9.1.0",
    "@cycle/http": "^8.2.2",
    "@cycle/isolate": "^1.2.0",
    "@panosoft/ramda-utils": "^0.2.8",
    "compression": "^1.6.1",
    "cropperjs": "^0.7.1",
    "cycle-local-storage": "git+https://github.com/TylorS/cycle-local-storage.git",
    "cyclic-router": "^1.0.0",
    "express": "^4.13.4",
    "history": "^2.0.1",
    "jquery": "^2.2.4",
    "moment": "^2.12.0",
    "normalize-css": "^2.3.1",
    "pm2": "^1.1.3",
    "ramda": "^0.21.0",
    "ramda-fantasy": "^0.5.0",
    "rx": "^4.1.0",
    "rx-combine-latest-obj": "^1.0.2"
  },
  "standard": {
    "ignore": [
      "/dist/"
    ],
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach"
    ]
  }
}
